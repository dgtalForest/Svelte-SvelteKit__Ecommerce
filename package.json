{
	"name": "svelte-ecom",
	"version": "0.0.2",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"generate": "drizzle-kit generate:sqlite",
		"db:push": "drizzle-kit push:sqlite --config=drizzle.config.ts",
		"db:studio": "drizzle-kit studio",
		"db:seed": "tsx seed.ts",
		"stripe:listen": "stripe listen --forward-to http://localhost:5173/api/stripe"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.1.0",
		"@sveltejs/adapter-vercel": "^4.0.4",
		"@sveltejs/kit": "^2.2.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.1",
		"@typescript-eslint/eslint-plugin": "^6.18.1",
		"@typescript-eslint/parser": "^6.18.1",
		"autoprefixer": "^10.4.16",
		"drizzle-kit": "^0.20.10",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-svelte": "^2.35.1",
		"postcss": "^8.4.33",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^4.2.8",
		"svelte-adapter-bun": "^0.5.1",
		"svelte-check": "^3.6.2",
		"tailwindcss": "^3.4.1",
		"tslib": "^2.6.2",
		"tsx": "^4.7.0",
		"typescript": "^5.3.3",
		"vite": "^5.0.11"
	},
	"type": "module",
	"dependencies": {
		"@libsql/client": "^0.3.6",
		"@lucia-auth/adapter-sqlite": "3.0.0-beta.9",
		"arctic": "^0.10.3",
		"better-sqlite3": "^9.2.2",
		"bits-ui": "^0.9.9",
		"clsx": "^2.1.0",
		"dotenv": "^16.3.1",
		"drizzle-orm": "^0.29.3",
		"lucia": "3.0.0-beta.11",
		"lucide-svelte": "^0.294.0",
		"oslo": "^0.24.0",
		"stripe": "^14.11.0",
		"svelte-cloudinary": "^1.1.2",
		"tailwind-merge": "^2.2.0",
		"tailwind-variants": "^0.1.20",
		"zod": "^3.22.4",
		"zod-form-data": "^2.0.2"
	}
}
